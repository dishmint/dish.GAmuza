angle = 0

topBox = box(ofVec3f(0.0,0.0,0.0),200)

function setup()
end

function update()
  angle = angle + 1
end

function draw()
  gaBackground(0.5 ,1.0)
  
  ofPushMatrix()
  ofTranslate(OUTPUT_W/2.0,OUTPUT_H/2.0,0.0)
  ofRotateX(angle)
  topBox:show()
  ofPopMatrix()
end
